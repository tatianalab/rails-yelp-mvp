<%= simple_form_for(restaurant) do |f| %>
    <div class = "form-group">
      <%= f.input :name %>
    </div>
    <div class = "form-group">
      <%= f.input :address %>
    </div>
    <div class = "form-group">
      <%= f.input :category %>
    </div>
    <div class = "form-group">
      <%= f.input :phone_number %>
    </div>
    <div class = "form-group">
      <%= f.submit class: 'btn btn-primary' %>
    </div>
    <% end %>
  </div>
